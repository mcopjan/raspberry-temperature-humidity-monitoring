<Application x:Class="Raspberry.Temperature.Humidity.WPF.Desktop.Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converts="clr-namespace:Raspberry.Temperature.Humidity.WPF.Desktop.Client.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <converts:DateConverter x:Key="DateConverter"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/UIColors.xaml"/>
                <ResourceDictionary Source="Styles/ButtonStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
